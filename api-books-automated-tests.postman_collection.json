			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Body contains expected fields\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"id\");\r",
							"    pm.expect(pm.response.text()).to.include(\"name\");\r",
							"    pm.expect(pm.response.text()).to.include(\"type\");\r",
							"    pm.expect(pm.response.text()).to.include(\"available\");\r",
							"});\r",
							"pm.test(\"Body contains expected type of data\", function () {\r",
							"    jsonData.forEach(function(item) {\r",
							"    pm.expect(item.id).to.be.a('number');\r",
							"    pm.expect(item.name).to.be.a('string');\r",
							"    pm.expect(item.type).to.be.a('string');\r",
							"    pm.expect(item.available).to.be.a('boolean');\r",
							"});\r",
							"});\r",
							"pm.test(\"Response time is less than 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							""
